block settings
    - var title = 'Experiência de Sucesso'
doctype
html(lang="pt")
    head
        include ../_partials/meta-tags
        <link rel="apple-touch-icon" href="icon.png"> 
        title #{title}
        block CSS
            link(rel="stylesheet", href="css/plugins.css")
            link(rel="stylesheet", href="css/style.css") 
    body
        block header
            section#topo
                header
                    .container                
                        .row
                            .col-sm-12.pd-20-top.pd-20-bottom
                                img(src="https://s3-sa-east-1.amazonaws.com/happybiz-web/Experiencia-De-Sucesso/novo-site/logo-es-branco.svg", alt="Logo ES", class="img-responsive", style="max-width:180px")
                    .navegacao.hidden-md
                        .container
                            .row
                                nav
                                    ul#desk-nav 
                                        each item in ['QUEM SOMOS', 'PALESTRANTES', 'BLOG', 'DEPOIMENTOS', 'AGENDA', 'QUERO SER UM AFILIADO', 'ÁREA DO CLIENTE', 'PERGUNTAS FREQUENTES', 'COACH ES', 'CONTATO']
                                            li 
                                                a(href="#") #{item}


        block content

        block scriptsJS
            script(src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js")
            script(src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js")

            